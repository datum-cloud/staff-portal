{
  "name": "staff-portal",
  "version": "0.0.1",
  "private": true,
  "sideEffects": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "bunx --bun vite",
    "lint": "bunx eslint \"{app,lib}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "start": "bun ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile --typescript",
    "test:e2e": "start-server-and-test 'bun run dev' http://localhost:3000 'bunx cypress run'",
    "test:e2e:prod": "start-server-and-test 'bun run start' http://localhost:3000 'bunx cypress run --quiet'",
    "test:e2e:debug": "start-server-and-test 'bun run start' http://localhost:3000 'bunx cypress open'",
    "test:unit:prod": "cypress run --component --quiet",
    "test:unit:debug": "cypress open --component"
  },
  "dependencies": {
    "@epic-web/client-hints": "^1.3.5",
    "@lingui/core": "^5.3.1",
    "@lingui/detect-locale": "^5.3.1",
    "@lingui/react": "^5.3.1",
    "@react-router/fs-routes": "^7.6.0",
    "@react-router/node": "^7.6.0",
    "accept-language-parser": "^1.5.0",
    "dotenv": "^16.5.0",
    "hono": "^4.7.9",
    "intl-parse-accept-language": "^1.0.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "react-router-hono-server": "^2.13.0"
  },
  "devDependencies": {
    "@lingui/vite-plugin": "^5.3.1",
    "@react-router/dev": "^7.6.0",
    "@tailwindcss/vite": "^4.1.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/accept-language-parser": "^1.5.8",
    "@types/bun": "^1.2.13",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "lefthook": "^1.11.12",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "resolutions": {
    "@mongodb-js/zstd": "^1.2.2"
  }
}
